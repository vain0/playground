{
  "name": "noupper-lang",
  "version": "0.1.0",
  "description": "noupper-lang support",
  "repository": "https://github.com/vain0x/noupper-lang",
  "author": "vain0x",
  "publisher": "vain0x",
  "license": "Unlicensed",
  "private": true,
  "main": "./out/extension",
  "activationEvents": [
    "onLanguage:noupper-lang"
  ],
  "engines": {
    "vscode": "^1.30.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "vsce": "^1.54.0",
    "vscode": "^1.1.26",
    "vscode-languageserver-protocol": "^3.14.1"
  },
  "dependencies": {
    "node-wasm": "^0.1.4",
    "vscode-languageclient": "^5.2.1"
  },
  "scripts": {
    "postinstall": "vscode-install",
    "build": "tsc -b",
    "watch": "tsc -w"
  },
  "contributes": {
    "languages": [
      {
        "id": "noupper-lang",
        "extensions": [
          ".noupper"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "noupper-lang config",
      "properties": {
        "noupper-lang.trace.server": {
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Trace noupper-lang LSP server.",
          "scope": "window"
        }
      }
    }
  }
}
