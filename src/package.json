{
  "name": "puqqing-lang",
  "version": "0.1.0",
  "description": "Dynamic purely functional language",
  "main": "index.js",
  "scripts": {
    "dev": "run-p bundle-watch serve test-watch",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "bundle": "webpack",
    "bundle-watch": "webpack --watch",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "serve": "node ../build/serve.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vain0x/puqqing-lang.git"
  },
  "author": "vain0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vain0x/puqqing-lang/issues"
  },
  "homepage": "https://github.com/vain0x/puqqing-lang#readme",
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/node": "^10.5.1",
    "@types/pegjs": "^0.10.1",
    "cross-env": "^5.2.0",
    "jest": "^23.3.0",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^4.15.0",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "pegjs": "^0.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
